{
  "name": "web-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:staged": "lint-staged",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier \"src/**/*.{ts,tsx}\"",
    "start:dev": "NODE_ENV=development webpack serve",
    "test": "npm run test:jest && npm run test:cypress",
    "test:jest": "jest",
    "test:watch": "jest --verbose --watchAll --no-cache --runInBand",
    "test:cypress": "cypress run",
    "tsc:check": "npm run tsc:src && npm run tsc:cypress",
    "tsc:src": "tsc --noEmit --project tsconfig.json",
    "tsc:cypress": "tsc --noEmit --project cypress/tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fontsource/roboto": "4.1.0",
    "@material-ui/core": "4.11.3",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@babel/runtime": "7.12.13",
    "@testing-library/react": "11.2.5",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.1",
    "cypress": "6.4.0",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "ts-jest": "26.5.0",
    "typescript": "4.1.3",
    "webpack": "5.20.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint",
      "git add"
    ]
  }
}
